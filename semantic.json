{
  "base": "src/public/semantic",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "container",
    "list",
    "icon",
    "breadcrumb",
    "form",
    "grid",
    "menu",
    "search",
    "dropdown",
    "visibility",
    "transition",
    "button",
    "divider",
    "label",
    "message",
    "popup"
  ],  
  "version": "2.3.1"
}